// Jenkinsfile
pipeline {
    agent any // Chạy trên bất kỳ agent nào có sẵn

    stages {
        stage('Hello') {
            steps {
                echo 'Chào bạn, đây là pipeline đầu tiên của tôi!'
            }
        }
        stage('Test') {
            steps {
                echo 'Đây là bước giả lập chạy test...'
            }
        }
    }
}
